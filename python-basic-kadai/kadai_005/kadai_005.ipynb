        "# 台形の辺の長さ\n",
        "top = 10   # 上辺（cm）\n",
        "bottom = 20  # 下辺（cm）\n",
        "height = 5   # 高さ（cm）\n",
        "\n",
        "# 台形の面積の公式\n",
        "area = (top + bottom) * height / 2\n",
        "\n",
        "print(f\"台形の面積は {area} cm² です。\")\n"
